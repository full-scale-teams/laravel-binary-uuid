{
    "name": "ctberdon/laravel-binary-uuid",
    "description": "Binary support for UUIDs in Laravel Forked from spatie/laravel-binary-uuid",
    "keywords": [
        "ctberdon",
        "spatie",
        "laravel-binary-uuid"
    ],
    "homepage": "https://github.com/ctberdon/laravel-binary-uuid",
    "license": "MIT",
    "authors": [
        {
            "name": "Brent Roose",
            "email": "brent@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.1|^8.2",
        "illuminate/queue": "~7.0|~8.0|~9.0|~10.0",
        "ramsey/uuid": "^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^7.0|^8.0|^9.0|^10.0",
        "scrutinizer/ocular": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\BinaryUuid\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\BinaryUuid\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\BinaryUuid\\UuidServiceProvider"
            ]
        }
    }
}
